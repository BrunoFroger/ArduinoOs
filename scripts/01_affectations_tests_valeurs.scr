	echo "c > b";
fi
if ($c >= $b)
	echo "c >= b";
fi
if ($c < $b)
	echo "c < b";
fi
if ($c <= $b)
	echo "c <= b";
fi

if ($c == $a )
	echo "c = a";
else
	echo "c different de a";
fi